{
    "name": "atanu-portfolio",
    "version": "1.0.0",
    "description": "Portfolio website for Atanu Roy",
    "scripts": {
        "build:css": "tailwindcss -i ./static/css/main.css -o ./static/css/style.css",
        "watch:css": "tailwindcss -i ./static/css/main.css -o ./static/css/style.css --watch",
        "build:css:prod": "tailwindcss -i ./static/css/main.css -o ./static/css/style.css --minify"
    },
    "keywords": [
        "portfolio",
        "tailwindcss"
    ],
    "author": "Atanu Roy",
    "license": "MIT",
    "devDependencies": {
        "tailwindcss": "^3.3.5"
    }
}